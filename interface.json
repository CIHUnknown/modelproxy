            "daily": "http://url3"
        },
        "ruleFile": "Cart.getCart.rule.json",
        "status": "mock",
        "isRuleStatic": true,
        "method": "post",
        "dataType": "json",
        "encoding": "utf8",
        "isCookieNeeded": true,
