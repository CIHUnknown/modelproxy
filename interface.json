{
    "title": "pad淘宝数据接口定义",
    "version": "1.0.0",
    "engine": "mockjs",
    "rulebase": "../interfaceRules/",
    "status": "online",
    "interfaces": [ {
        "name": "detail",
        "id": "Detail.get",
        "urls": {
